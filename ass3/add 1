#include <iostream>
#include <vector>
#include <stack>
using namespace std;

vector<int> nearestSmallerLeft(const vector<int>& arr) {
    stack<int> s;
    vector<int> result;
    for (int x : arr) {
        while (!s.empty() && s.top() >= x) {
            s.pop();
        }
        if (s.empty()) result.push_back(-1);
        else result.push_back(s.top());
        s.push(x);
    }
    return result;
}

int main() {
    vector<int> arr = {1, 3, 0, 2, 5};
    vector<int> res = nearestSmallerLeft(arr);
    for (int x : res) cout << x << " ";
    cout << endl;
    return 0;
}
